<%- include("header") -%>
    <div class="box" id="heading">
       <h1><%=listTitle%></h1>
    </div>
  <ul>
    <div class="box"> 

     <% for(var i=0;i<newListItems.length;i++){ %>
        <div class="item">
            <input type="checkbox">
            <p><%= newListItems[i] %></p>
        </div>
     <%}%> 
    
      <form class="form" action="/" method="post">
        <input type="text" name="additem" placeholder="new-item" autocomplete="off">
        <button type="submit" name="list" value=<%=listTitle%>>+</button> 
      </form>
    </div>  

<%- include("footer") -%>




